{
  "name": "bemyslavedarlin/banish",
  "description": "Service for telegram ban bot",
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/dbal": "^3",
    "doctrine/doctrine-bundle": "^2.10",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.16",
    "dragonmantank/cron-expression": "^3.3",
    "php-amqplib/php-amqplib": "^3.2",
    "phpdocumentor/reflection-docblock": "^5.4",
    "phpstan/phpdoc-parser": "^1.28",
    "ramsey/uuid": "^4.7",
    "scienta/doctrine-json-functions": "^5.3",
    "symfony/amqp-messenger": "6.4.*",
    "symfony/cache": "6.4.*",
    "symfony/config": "6.4.*",
    "symfony/console": "6.4.*",
    "symfony/doctrine-messenger": "6.4.*",
    "symfony/dotenv": "6.4.*",
    "symfony/flex": "^1.17|^2",
    "symfony/framework-bundle": "6.4.*",
    "symfony/http-client": "6.4.*",
    "symfony/messenger": "6.4.*",
    "symfony/mime": "6.4.*",
    "symfony/monolog-bundle": "^3.8",
    "symfony/property-access": "6.4.*",
    "symfony/property-info": "6.4.*",
    "symfony/routing": "6.4.*",
    "symfony/runtime": "6.4.*",
    "symfony/scheduler": "6.4.*",
    "symfony/security-bundle": "6.4.*",
    "symfony/serializer": "6.4.*",
    "symfony/validator": "6.4.*",
    "symfony/yaml": "6.4.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-doctrine": "^1.4",
    "phpstan/phpstan-symfony": "^1.3",
    "squizlabs/php_codesniffer": "^3.8",
    "roave/security-advisories": "dev-latest",
    "symfony/maker-bundle": "^1.50"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "analyze": [
      "@phpstan"
    ],
    "phpstan": "phpstan analyse --level 9 --memory-limit=2G",
    "phpstan-baseline": "phpstan analyse --level 9 --generate-baseline var/phpstan-baseline.neon --memory-limit=2G",
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "quality": [
      "@phpstan",
      "@cs-check"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.4.*"
    }
  }
}
